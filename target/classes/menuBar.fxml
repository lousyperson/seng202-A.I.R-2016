<MenuBar xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng202.group4.GUI.MenuBarController">
    <menus>
        <Menu mnemonicParsing="false" text="File">
            <items>
                <Menu mnemonicParsing="false" text="Import">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#loadAirline" text="Airline" />
                        <MenuItem mnemonicParsing="false" onAction="#loadAirport" text="Airport" />
                        <MenuItem mnemonicParsing="false" onAction="#loadRoute" text="Route" />
                        <MenuItem mnemonicParsing="false" onAction="#loadFlight" text="Flight" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Reset to Default">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#resetAirline" text="Airline" />
                        <MenuItem mnemonicParsing="false" onAction="#resetAirport" text="Airport" />
                        <MenuItem mnemonicParsing="false" onAction="#resetRoute" text="Route" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Delete all">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#deleteAllAirlines" text="Airline" />
                        <MenuItem mnemonicParsing="false" onAction="#deleteAllAirports" text="Airport" />
                        <MenuItem mnemonicParsing="false" onAction="#deleteAllRoutes" text="Route" />
                        <MenuItem mnemonicParsing="false" onAction="#deleteAllFlights" text="Flight" />
                    </items>
                </Menu>
            </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <items>
                <MenuItem mnemonicParsing="false" onAction="#getHelp" text="A.I.R. Help" />
            </items>
        </Menu>
    </menus>
</MenuBar>
